{
  "name": "gulp-sugar-srcset",
  "version": "0.1.1",
  "description": "To set the contents of the srcset.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tsuguya/gulp-sugar-srcset"
  },
  "scripts": {
    "test": "tape 'test/**/*.js'",
    "test-config": "tape 'test/config.js' || true",
    "test-common": "tape 'test/common.js' | tap-diff || true",
    "test-resolution": "tape 'test/resolution/*.js' | tap-diff || true",
    "test-resolution-src": "tape 'test/resolution/src.js' | tap-diff || true",
    "test-resolution-srcset": "tape 'test/resolution/srcset.js' | tap-diff || true",
    "test-responsive": "tape 'test/width/*.js' | tap-diff || true",
    "test-responsive-src": "tape 'test/width/src.js' | tap-diff || true",
    "test-responsive-srcset": "tape 'test/width/srcset.js' | tap-diff || true"
  },
  "keywords": [
    "srcset"
  ],
  "author": "Tsuguya Toma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Tsuguya/gulp-sugar-srcset/issues"
  },
  "homepage": "https://github.com/Tsuguya/gulp-sugar-srcset",
  "dependencies": {
    "cheerio": "^0.22.0",
    "gulp-util": "^3.0.7",
    "through2": "^2.0.1"
  },
  "devDependencies": {
    "tap-diff": "^0.1.1",
    "tape": "^4.6.2"
  }
}
